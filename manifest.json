{
    "manifest_version": 3,
    "name": "Swedbank add on",
    "version": "1.0.0",
    "author": "Eimantas Melnikas",
    "description": "Ability to change your swedbank profile.",
    "permissions": ["storage"],
    "icons": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    },
    "content_scripts": [
      {
        "matches": ["https://www.swedbank.lt/*"],
        "js": ["./dist/content_bundle.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html"
    }
  }
  